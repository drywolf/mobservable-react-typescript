
{
	"version": "0.1.0",
	"command": "npm",
	"isShellCommand": true,
	"showOutput": "always",  
  "args": ["run"],  
  "tasks": [
        {
            "taskName": "build",
            "suppressTaskName": true,
            "args": ["build"],
            "isBuildCommand": true,
            "problemMatcher": "$tsc"
        },
        {
            "taskName": "run",
            "suppressTaskName": true,
            "args": ["start"],
            "isBuildCommand": false
        }
    ]
}
